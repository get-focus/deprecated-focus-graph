{
    "name": "focus-graph",
    "version": "3.2.2",
    "description": "Form, data behaviours",
    "main": "src/index.js",
    "author": "FOCUS <focus@kleegroup.com>",
    "license": "MIT",
    "files": [
        "api",
        "actions",
        "behaviours",
        "components",
        "dist",
        "index.js",
        "LICENSE",
        "middlewares",
        "package.json",
        "README.md",
        "reducers",
        "scripts",
        "server.js",
        "src",
        "store",
        "style",
        "test",
        "webpack.config.js"
    ],
    "bugs": {
        "url": "https://github.com/get-focus/focus-graph/issues"
    },
    "homepage": "https://github.com/get-focus/focus-graph#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/get-focus/focus-graph.git"
    },
    "scripts": {
        "build": "npm run build:node",
        "build:node": "better-npm-run copy-sass && better-npm-run babelify",
        "start": "better-npm-run dev-server",
        "test": "BABEL_ENV=test mocha src/**/__tests__/**/*.js",
        "test:watch": "BABEL_ENV=test mocha src/**/__tests__/**/*.js --watch",
        "test-windows:watch": "better-npm-run test",
        "prepublish": "npm run build"
    },
    "betterScripts": {
        "copy-sass": {
            "command": "cd src && find . -name *.scss | xargs tar cvf - | tar xfp - -C ../"
        },
        "test": {
            "command": "mocha src/**/__tests__/**/*.js --watch",
            "env": {
                "BABEL_ENV": "test",
                "DEV": true,
                "SOURCE_MAPS": false,
                "ENTRY_FILE_PATH": "./src/example/index",
                "PAGE_TITLE": "Focus entity dev page",
                "MINIMIFY": false,
                "API_PORT": 9999,
                "PACKAGE_JSON_PATH": "../",
                "ANCHOR_CLASS": "entity-example"
            }
        },
        "dev-server": {
            "command": "node ./server.js",
            "env": {
                "DEV": true,
                "SOURCE_MAPS": false,
                "ENTRY_FILE_PATH": "./src/example/index",
                "PAGE_TITLE": "Focus entity dev page",
                "MINIMIFY": false,
                "API_PORT": 9999,
                "PACKAGE_JSON_PATH": "../",
                "ANCHOR_CLASS": "entity-example"
            }
        },
        "babelify": {
            "env": {
                "BABEL_ENV": "production"
            },
            "command": "node scripts/babelify.js"
        }
    },
    "keywords": [
        "react",
        "redux",
        "data",
        "focus"
    ],
    "peerDependencies": {
        "i18next": ">= 4.1.0",
        "lodash": ">= 4.15.0",
        "react": ">= 15.3.0 < 16.0.0",
        "react-dom": ">= 15.3.0 < 16.0.0",
        "react-redux": ">= 4.4.5",
        "redux": ">= 3.5.2",
        "redux-thunk": ">= 2.1.0",
        "moment": ">= 2.14.1"
    },
    "devDependencies": {
        "autoprefixer-loader": "^3.2.0",
        "babel-core": "^6.7.6",
        "babel-eslint": "^6.0.2",
        "babel-loader": "^6.2.4",
        "babel-plugin-flow-comments": "^6.3.19",
        "babel-polyfill": "^6.5.0",
        "babel-preset-focus": "^0.6.1",
        "better-npm-run": "0.0.6",
        "body-parser": "^1.14.2",
        "chai": "^3.5.0",
        "chai-subset": "^1.2.0",
        "css-loader": "^0.23.1",
        "express": "^4.13.4",
        "extract-text-webpack-plugin": "^1.0.1",
        "faker": "^3.0.1",
        "file-loader": "^0.8.5",
        "html-webpack-plugin": "^2.8.1",
        "i18next": "^4.1.0",
        "isomorphic-fetch": "^2.2.1",
        "jsdom": "^8.3.1",
        "json-loader": "^0.5.4",
        "lodash": "^4.15.0",
        "material-design-icons-iconfont": "^2.0.4",
        "material-design-lite": "^1.1.1",
        "mocha": "^2.4.5",
        "moment": "^2.14.1",
        "node-sass": "^3.4.2",
        "react": "15.4.1",
        "react-dom": "15.4.1",
        "react-addons-test-utils": "15.4.1",
        "react-hot-loader": "^1.3.0",
        "react-redux": "^4.4.5",
        "react-router": "^2.8.1",
        "redux": "^3.6.0",
        "redux-devtools": "^3.3.1",
        "redux-devtools-dispatch": "^2.2.0",
        "redux-devtools-dock-monitor": "^1.1.1",
